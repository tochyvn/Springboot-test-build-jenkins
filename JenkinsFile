pipeline {

    agent any
    tools { 
        maven 'Maven 3.3.9' 
        jdk 'jdk8' 
    }
    stages {

    stage('Build') {
	    steps {
    	    script {
		 image_name = "overview-project"
		 archive_name = "${image_name}.tgz"
                 sh "sudo ls -la"
                 sh "java -version"
		 sh "mvn --version"
		 echo "PATH = ${PATH}"
                 echo "M2_HOME = ${M2_HOME}"
		
	        }
   	        
        }
    }

    stage('Clean') {
	 steps {
		script {
			sh "sudo ls -la"
			}
		}
	}
}
    post {
	always {
		cleanWs()
		}
	}
}
