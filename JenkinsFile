pipeline {

    agent any

    stages {

    stage('Build') {
	    steps {
    	    script {
		 image_name = "overview-project"
		 archive_name = "${image_name}.tgz"
                 sh "sudo ls -la"
                 sh "java -version"
		 sh "sudo docker ps"
	        }
   	        archiveArtifacts artifacts: "${archive_name}"
        }
    }

    stage('Clean') {
	 steps {
		script {
			sh "sudo ls -la"
			}
		}
	}
}
    post {
	always {
	     mvn --version
		cleanWs()
		}
	}
}
