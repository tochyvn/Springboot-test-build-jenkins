pipeline {

    agent any
    tools { 
        jdk 'JDK8u212' 
        maven 'Maven' 
    }
    stages {

    stage('Build') {
	    steps {
    	    script {
		 image_name = "overview-project"
		 archive_name = "${image_name}.tgz"
                 sh "sudo ls -la"
                 sh "java -version"
		 sh "mvn --version"
		 sh 'echo "PATH = ${PATH}"'
                 sh 'echo "M2_HOME = ${M2_HOME}"'
		
	        }
   	        
        }
    }

    stage('Clean') {
	 steps {
		script {
			sh "sudo ls -la"
			}
		}
	}
}
    post {
	always {
		cleanWs()
		}
	}
}
